  <appSettings>
    <add key="MinimumMatchesRequired" value="25" />
    <add key="MatchUniquenessThreshold" value="0.8" />
    <add key="MatchDistanceThreshold" value="80" />
    <add key="FeatureExtractor" value="orb" />
    <add key="MatchFilter" value="simple" />
    <add key="Evaluate" value="true" />
    <add key="Cache" value="true" />
  </appSettings>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source = |SQL/CE|" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="ServiceBehavior" name="ArtifactDetector.Services.DetectorService">
        <endpoint address="DetectorService" binding="netNamedPipeBinding" contract="ArtifactDetector.Services.IDetectorService" />
        <endpoint address="DetectorService/mex" binding="mexNamedPipeBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.pipe://localhost/itsape/artifact-detector/v1.0/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ServiceBehavior">
          <serviceMetadata httpGetEnabled="False" />
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
